{
  "name": "contributte/mailing",
  "description": "Sending emails with pleasure and prepared templates.",
  "keywords": [
    "nette",
    "mail",
    "mailing"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/mailing",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "nette/application": "~3.0.0",
    "nette/di": "~3.0.0",
    "nette/mail": "~3.0.1",
    "latte/latte": "~2.5.1"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.9",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Mailing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Mailing\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
