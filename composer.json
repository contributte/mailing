{
  "name": "contributte/mailing",
  "description": "Sending emails with pleasure and prepared templates.",
  "keywords": [
    "nette",
    "mail",
    "mailing"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/mailing",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/application": "^3.2.7",
    "nette/di": "^3.2.4",
    "nette/mail": "^4.0.3",
    "latte/latte": "^3.0.23"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.3",
    "contributte/qa": "^0.4.0",
    "contributte/phpstan": "^0.2.0",
    "contributte/tester": "^0.4.0"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Mailing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Mailing\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
